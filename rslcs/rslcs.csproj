<Project Sdk="Microsoft.NET.Sdk">
	<PropertyGroup>
		<TargetFramework>net8.0</TargetFramework>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>enable</Nullable>
		<GeneratePackageOnBuild>True</GeneratePackageOnBuild>
		<Title>rslcs</Title>
		<Description>Lightweight C# library for Go-like error handling</Description>
		<Copyright>Copyright © 2025 Fynn Budde</Copyright>
		<PackageId>rslcs</PackageId>
		<Version>1.0.0</Version>
		<Product>rslcs</Product>
		<Authors>Fynn Budde</Authors>
		<PackageProjectUrl>https://github.com/Foib/rslcs</PackageProjectUrl>
		<PackageIcon>icon.png</PackageIcon>
		<PackageReadmeFile>README.md</PackageReadmeFile>
		<RepositoryUrl>https://github.com/Foib/rslcs</RepositoryUrl>
		<RepositoryType>git</RepositoryType>
		<PackageTags>result;error handling;go error;go</PackageTags>
		<PackageLicenseFile>LICENSE.txt</PackageLicenseFile>
	</PropertyGroup>
	<ItemGroup>
		<None Update="LICENSE.txt">
			<Pack>True</Pack>
			<PackagePath>\</PackagePath>
		</None>
		<None Update="icon.png">
			<Pack>True</Pack>
			<PackagePath>\</PackagePath>
		</None>
		<None Update="README.md">
			<Pack>True</Pack>
			<PackagePath>\</PackagePath>
		</None>
	</ItemGroup>
</Project>
